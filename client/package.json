    "gh-pages": "^4.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.3",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.1.3",
    "typescript": "4.7.3",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "homepage": "https://vasiliiperfilev.github.io/shop"
